#!/bin/sh
# postrm script for openscap-daemon
#
# see: dh_installdeb(1)

set -e

case "$1" in
  purge)
    # due to generated pyc in the cve_scanner subdir of openscap_daemon
    # python dist-packages dir, the purge is not properly made automatically
    rm -rf /usr/lib/python2.7/dist-packages/openscap_daemon
    ;;  
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
